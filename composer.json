{
  "name": "wavevision/di-service-annotation",
  "description": "Register DI services using annotation for Nette Framework.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Wavevision s.r.o.",
      "email": "info@wavevision.com"
    },
    {
      "name": "Jakub Filla",
      "email": "jakub.filla@wavevision.com"
    },
    {
      "name": "Vít Rozsíval",
      "email": "vit@wavevision.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Wavevision\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wavevision\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "sort-packages": true
  },
  "require": {
    "doctrine/annotations": "^1.8",
    "nette/finder": "^2.5",
    "php": ">=7.4",
    "wavevision/utils": "^2.2"
  },
  "require-dev": {
    "jakub-onderka/php-parallel-lint": "^1.0",
    "mikey179/vfsstream": "^1.6",
    "phing/phing": "^3.0.0a3",
    "php-coveralls/php-coveralls": "^2.1",
    "phpstan/phpstan": "^0.11",
    "phpunit/phpunit": "^8.3",
    "wavevision/coding-standard": "^3.1"
  },
  "scripts": {
    "phing": "phing"
  }
}
